<Window x:Class="Deplonium.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Deplonium"
        mc:Ignorable="d"
        xmlns:System="clr-namespace:System;assembly=mscorlib"
        xmlns:Service="clr-namespace:Deplonium.Model"
        Title="MainWindow" Height="350" Width="525" Loaded="Window_Loaded">
    <Grid>
        <Button Content="Button" HorizontalAlignment="Left" Margin="10,289,0,0" VerticalAlignment="Top" Width="75"/>
        <ComboBox x:Name="CbxServiceInstanceType" HorizontalAlignment="Left" Margin="10,236,0,0" VerticalAlignment="Top" Width="120" ItemsSource="{Binding ServiceTypes}">
        </ComboBox>
    </Grid>
</Window>
